    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-vue": "^7.1.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5",
    "vite": "^1.0.0-rc.8",
    "vue-type-check": "git+https://github.com/dora1998/vue-type-check.git#dist"
  }
